{
    "compilerOptions": {
        "outDir": "./build/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "strict": true,
        "esModuleInterop": true
    }
}
