  },
  "homepage": "https://github.com/thaumant/lucid-suggest#readme",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-wasm": "^3.0.0",
    "benchmark": "^2.1.4",
    "jest": "^25.1.0",
    "rollup": "^1.31.1"
  }
}
